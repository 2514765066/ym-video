{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": ["electron.vite.config.*", "src/**/*"],
  "exclude": ["src/renderer", "src/preload"],
  "compilerOptions": {
    "composite": true,
    "types": ["electron-vite/node"],
    "paths": {
      "@/*": ["./src/main/*"],
      "@type": ["./src/type/index"]
    }
  }
}
